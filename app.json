{
  "expo": {
    "name": "Cante Uma Nova Canção",
    "slug": "AnthonyOliveira",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/note_logo.png",
    "splash": {
      "image": "./assets/splash_logo.png",
      "resizeMode": "cover"
    },
    "updates": {
