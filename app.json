      "bundleIdentifier": "com.anthony.dev.AnthonyOliveira"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/note_logo.png"
      },
      "package": "com.anthony.dev.AnthonyOliveira"
    },
    "web": {
      "favicon": "./assets/note_logo.png"
    }
  }
}
