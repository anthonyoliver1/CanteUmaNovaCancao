{
  "name": "canteumanovacancao",
  "version": "1.0.0",
  "scripts": {
    "start": "npm install && expo start --dev-client",
    "android": "expo run:android --device",
    "ios": "expo run:ios --device",
    "web": "expo start --web",
    "eject": "expo eject",
    "localhost": "npm install && npx expo start --localhost",
    "lan": "npm install && npx expo start --lan",
    "tunnel": "npm install && npx expo start --tunnel",
    "build:android": "eas build --platform android",
    "build:ios": "eas device:create && eas build --platform ios",
    "build:apk": "eas build -p android --profile preview",
    "build:dev-android": "eas build --profile development --platform android",
    "build:android-develop-apk": "npm run clean-module && npm install && npx expo-doctor && npm install && npx expo prebuild && cd android && ./gradlew clean && ./gradlew :app:assembleRelease && cd ../",
    "build:android-apk": "npm run clean-module && npm install && npx expo-doctor && npm install && npx expo prebuild && cd android &&./gradlew clean && ./gradlew :app:bundleRelease && cd ../",
    "build:dev-ios": "eas build --profile development-simulator --platform ios",
    "clean-module": "rm -rf node_modules package-lock.json && npm cache clean --force",
    "dependecies-in-use": "npx depcheck"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "expo": "~48.0.18",
    "expo-av": "~13.2.1",
    "expo-clipboard": "~4.1.2",
    "expo-dev-client": "~2.2.1",
    "expo-file-system": "~15.2.2",
    "expo-haptics": "~12.2.1",
    "expo-navigation-bar": "~2.1.1",
    "expo-network": "~5.2.1",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.4",
    "expo-web-browser": "~12.1.1",
    "moti": "^0.25.4",
    "qs": "^6.10.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.8",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-modalize": "^2.1.1",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-toast-notifications": "^3.3.1",
    "react-native-web": "~0.18.10",
    "react-native-webview": "11.26.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/ngrok": "^4.1.1",
    "@types/styled-components-react-native": "^5.1.3"
  },
  "private": true
}
